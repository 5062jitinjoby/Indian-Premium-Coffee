    <script>
        const qtyup = document.querySelectorAll('.qtyup')
        const qtydown = document.querySelectorAll('.qtydown')
        const qtyval = document.querySelectorAll('.qtyval')
        
        qtyup.forEach(el=>{
            el.addEventListener('click',async ()=>{
                const pid = el.dataset.proid
               let funcdata = await inc(pid)
               if(funcdata){
                const currentcont = el.closest('.detail-qty') 
                const currentval = parseInt(currentcont.querySelector('.qtyval').innerHTML)
                const currentPrice = parseInt(document.querySelector(`.pprice${pid}`).innerHTML)
                console.log(currentPrice)
                const currentTotal = document.querySelector(`.cost${pid}`)
                const total = currentPrice * (currentval + 1)
                currentTotal.innerHTML = total
                currentcont.querySelector('.qtyval').innerHTML = currentval + 1
                console.log(total)
               }
            })
        })
        qtydown.forEach(el=>{
            el.addEventListener('click',async ()=>{
                const pid = el.dataset.proid
               let funcdata = await dec(pid)
               if(funcdata){
                const currentcont = el.closest('.detail-qty') 
                const currentval = parseInt(currentcont.querySelector('.qtyval').innerHTML)
                const currentPrice = parseInt(document.querySelector(`.pprice${pid}`).innerHTML)
                console.log(currentPrice)
                const currentTotal = document.querySelector(`.cost${pid}`)
                const total = currentPrice * (currentval - 1)
                currentTotal.innerHTML = total
                currentcont.querySelector('.qtyval').innerHTML = currentval - 1
                console.log(total)
               }
            })
        })
        
        function inc(id){
            console.log('increment')
           return fetch(`/inc?id=${id}`,{
                method:'POST',
                headers:{
                    'Content-Type':'application/json',
                }
            })
            .then(Response=>{
                
                return Response.json()
            })
            .then(data=>{
                console.log(data.success)
                if(data.success){
                    return true
                }
            })
            .catch(error=>{
                console.log(error)
            })
        }

        function dec(id){
