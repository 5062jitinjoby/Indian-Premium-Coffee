<header class="header-area header-style-3 header-height-2">
    <div class="header-top header-top-ptb-1 d-none d-lg-block">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-3 col-lg-4">
                    <div class="header-info">
                        <ul>
                            <li><i class="fi-rs-smartphone"></i> <a href="#">(+01) - 2345 - 6789</a></li>
                            <li><i class="fi-rs-marker"></i><a  href="#">Our location</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-4">
                    <div class="text-center">

                    </div>
                </div>
                <div class="col-xl-3 col-lg-4">
                    <div class="header-info header-info-right">
                        <ul>
                            <li>
                                <a class="language-dropdown-active" href="#"> <i class="fi-rs-world"></i> English <i class="fi-rs-angle-small-down"></i></a>
                                <ul class="language-dropdown">
                                    <li><a href="#"><img src="/public/user/imgs/theme/flag-fr.png" alt="">Français</a></li>
                                    <li><a href="#"><img src="/public/user/imgs/theme/flag-dt.png" alt="">Deutsch</a></li>
                                    <li><a href="#"><img src="/public/user/imgs/theme/flag-ru.png" alt="">Pусский</a></li>
                                </ul>
                            </li>
                            <%if(locals.user){%>
                                <ul class="dropdown">
                                    <li><a href="/profile"><i class="fi-rs-user"></i><%= user.username%></a></li>
                                    <li><a href="/logout">Logout</a></li>
                                </ul>
                            <%}
                            else{%>
                                <ul class="dropdown">
                                    <li><a href="/login">Login</a></li>
                                </ul>
                            <%}%>
                            

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="header-middle header-middle-ptb-1 d-none d-lg-block">
        <div class="container">
            <div class="header-wrap">
                <div class="logo logo-width-1">
                    <a href="#"><h3>Indian Premium Coffee</h3></a>
                </div>
                <div class="header-right">
                    <div class="search-style-2">
                        <form action="/home/category" method="get">
                            <select class="select-active" name="selectedCategory">
                                <option>All Categories</option>
                                <%if(locals.category){ %>
                                    <% category.forEach((categor)=>{ %>
                                        <option value="<%= categor._id%>"><%= categor.name%></option>
                                    <% }) %>
                                <%  } %>
                            </select>
                            <input type="text" placeholder="Search for items...">
                            <button type="submit">Filter</button>
                        </form>
                    </div>
                    <div class="header-action-right">
                        <div class="header-action-2">
                            <div class="header-action-icon-2">
                                <a href="/wishlist">
                                    <img class="svgInject" alt="Evara" src="/public/user/imgs/theme/icons/icon-heart.svg">
                                    <span class="pro-count blue"></span>
                                </a>
                            </div>
                            <div class="header-action-icon-2">
                                <a class="mini-cart-icon" href="/wallet">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-wallet" viewBox="0 0 16 16">
                                        <path d="M0 3a2 2 0 0 1 2-2h13.5a.5.5 0 0 1 0 1H15v2a1 1 0 0 1 1 1v8.5a1.5 1.5 0 0 1-1.5 1.5h-12A2.5 2.5 0 0 1 0 12.5zm1 1.732V12.5A1.5 1.5 0 0 0 2.5 14h12a.5.5 0 0 0 .5-.5V5H2a2 2 0 0 1-1-.268M1 3a1 1 0 0 0 1 1h12V2H2a1 1 0 0 0-1 1"/>
                                    </svg>
                                </a>
                            </div>
                            <div class="header-action-icon-2">
                                <a class="mini-cart-icon" href="/cart">
                                    <img alt="Evara" src="/public/user/imgs/theme/icons/icon-cart.svg">
                                    <span class="pro-count blue"></span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="header-bottom header-bottom-bg-color sticky-bar">
        <div class="container">
            <div class="header-wrap header-space-between position-relative  main-nav">
                <div class="logo logo-width-1 d-block d-lg-none">
                    <a href="#"><h1>Indian Premium Coffee</h1></a>
                </div>
                <div class="header-nav d-none d-lg-flex">
                    <div class="main-categori-wrap d-none d-lg-block">
                        <a class="categori-button-active" href="#">
                            <span class="fi-rs-apps"></span> Browse Categories
                        </a>    
                    </div>
                    <div class="main-menu main-menu-padding-1 main-menu-lh-2 d-none d-lg-block">
                        <nav>
                            <ul>
                                <li><a class="" href="/home">Home</a>   
                                </li>
                                <!-- <li>
                                    <a href="">About</a>
                                </li>
                                <li><a href="">Shop</a>
                                </li>
                                <li class="position-static"><a href="#">Mega menu</a>
                                </li>
                                <li><a href="">Blog</a>
                                </li>
                                <li><a href="#">Pages</a>
                                </li>
                                <li>
                                    <a href="">Contact</a>
                                </li> -->
                            </ul>
                        </nav>
                    </div>
                </div>
                <div class="hotline d-none d-lg-block">
                    <p><i class="fi-rs-headset"></i><span>Hotline</span> 1900 - 888 </p>
                </div>
            </div>
        </div>
    </div>
</header>
