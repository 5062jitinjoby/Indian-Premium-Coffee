        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Flavours</h2>
                    <p>Add, edit or delete a flavour</p>
                </div>
                <div>
                    <input type="text" placeholder="Search Categories" class="form-control bg-white">
                </div>
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-3">
                            <form method="post" action="/admin/edit_flavours?_method=PUT" onsubmit="validateForm(event)">
                                <input type="hidden" name="_method" value="PUT">
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Name</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="flavourName" onkeyup="validateName()" value="<%= flavour.name%>" name="name" />
                                    <span class="error text tex-danger"  id="flavourError"></span>
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Description</label>
                                    <textarea placeholder="Type here" class="form-control" id="description" onkeyup="validateDescription()" value="<%= flavour.description%>" name="description"><%= flavour.description%></textarea>
                                    <span class="error text tex-danger"  id="desError"></span>
                                </div>
                                <div class="d-grid">
                                    <button class="btn btn-primary">Update Flavour</button>
                                </div>
                            </form>
                            <span class="text-danger" id="submitError"> </span>
                        </div>
                    </div> <!-- .row // -->
                </div> <!-- card body .// -->
            </div> <!-- card .// -->
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
