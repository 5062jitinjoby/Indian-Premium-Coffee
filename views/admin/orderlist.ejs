        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                </div>
            </div>
            <div class="row">
                <div class="col-md-9">
                    <div class="card mb-4">
                        <header class="card-header">
                            <div class="row gx-3">
                                <div class="col-lg-4 col-md-6 me-auto">
                                    <input type="text" placeholder="Search..." class="form-control">
                                </div>
                                <div class="col-lg-2 col-md-3 col-6">
                                    <select class="form-select">
                                        <option>Status</option>
                                        <option>Active</option>
                                        <option>Disabled</option>
                                        <option>Show all</option>
                                    </select>
                                </div>
                                <div class="col-lg-2 col-md-3 col-6">
                                    <select class="form-select">
                                        <option>Show 20</option>
                                        <option>Show 30</option>
                                        <option>Show 40</option>
                                    </select>
                                </div>
                            </div>
                        </header> <!-- card-header end// -->
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            
                                            <th>Customer name</th>
                                            <th>Product name</th>
                                            <th>Price</th>
                                            <th>Status</th>
                                            <th>Date</th>
                                            <th class="text-end"> Action </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%orders.forEach(items=>{%>
                                            <%items.orders.forEach(item=>{%>
                                                <%item.products.forEach(el=>{%>
                                                    <tr>
                                                        <td><b><%= items.userID.username%></b></td>
                                                        <td><%= el.productID.ProductName%></td>
                                                        <td>$<%= el.cost%></td>
                                                        <td>
                                                            <%if(el.status == 'Canceled'){%>
                                                                <span class="badge rounded-pill alert-danger"><%= el.status%></span>
                                                            <%}
                                                            else{%>
                                                                <span class="badge rounded-pill alert-success"><%= el.status%></span>
                                                            <%}%>

                                                        </td>
                                                        <td><%= (new Date(item.date)).toLocaleDateString('en-GB') %></td>
                                                        <td class="text-end">
                                                            
                                                            <div class="dropdown">
                                                                <a href="#" data-bs-toggle="dropdown"  class="btn btn-md rounded font-sm">Edit</a>
                                                                <div class="dropdown-menu">
                                                                    <a class="dropdown-item" href="/admin/orders/status?id=<%= items.userID._id%>&prid=<%= el._id%>&status=Placed" name="Placed">Placed</a>
                                                                    <a class="dropdown-item" href="/admin/orders/status?id=<%= items.userID._id%>&prid=<%= el._id%>&status=Pending" name="Pending">Pending</a>
                                                                    <a class="dropdown-item" href="/admin/orders/status?id=<%= items.userID._id%>&prid=<%= el._id%>&status=Delivered" name="Delivered">Delevered</a>
                                                                    <a class="dropdown-item text-danger" href="/admin/orders/status?id=<%= items.userID._id%>&prid=<%= el._id%>&status=Canceled" name="Cancel">Cancel</a>
                                                                </div>
                                                            </div> <!-- dropdown //end -->
                                                        </td>
                                                    </tr>
                                                <%})%>
                                            <%})%>    
                                        <%})%>
                                        
                                    </tbody>
                                </table>
                            </div> <!-- table-responsive //end -->
                        </div> <!-- card-body end// -->
                    </div> <!-- card end// -->
                </div>
                <div class="col-md-3">
                    <div class="card mb-4">
                        <div class="card-body">
                            <h5 class="mb-3">Filter by</h5>
                            <form>
                                <div class="mb-4">
                                    <label for="order_id" class="form-label">Order ID</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="order_id">
                                </div>
                                <div class="mb-4">
                                    <label for="order_customer" class="form-label">Customer</label>
