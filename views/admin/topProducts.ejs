            <div class="card mb-4">
                <header class="card-header">
                    <h4 class="card-title">Top 10 Selling Products</h4>
                </header>
                <div class="card-body">
                    <div class="table-responsive">
                        <div class="table-responsive">
                            <table class="table align-middle table-nowrap mb-0">
                                <thead class="table-light">
                                    <tr>
                                        <th class="align-middle" scope="col">Selling Rank</th>
                                        <th class="align-middle" scope="col">Product Name</th>
                                        <th class="align-middle" scope="col">Price</th>
                                        <th class="align-middle" scope="col">Category</th>
                                        <th class="align-middle" scope="col">Offer Price</th>
                                    </tr>
                                </thead>
                                <tbody id="defaultReport">
                                    <%for(let i=0; i<10; i++){%>
                                        <tr>
                                            <td class="fw-bold"><%= i+1%></td>
                                            <td><%= topSellingProducts[i].ProductName%></td>
                                            <td><%= topSellingProducts[i].price%></td>
                                            <td><%= topSellingProducts[i].category.name%></td>
                                            <%if(topSellingProducts[i].offerPrice!=0){%>
                                                <td><%= topSellingProducts[i].offerPrice%></td>
                                            <%}
                                            else{%>
                                                <td>-</td>
                                            <%}%>
                                        </tr>
                                    <%}%>
                                </tbody>
                            </table>
                        </div>
                    </div> <!-- table-responsive end// -->
